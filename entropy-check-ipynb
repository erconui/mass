   "source": [
    "def fill_prediction(ent):\n",
    "    for idx in range(1,len(ent)):\n",
    "        if np.isnan(ent[idx]):\n",
    "            for jdx in range(idx,len(ent)):\n",
    "                if not np.isnan(ent[jdx]):\n",
    "                    slope = (ent[jdx]- ent[idx-1])/(jdx-(idx-1))\n",
    "                    for kdx in range(idx, jdx):\n",
    "                        ent[kdx] = ent[idx-1] + slope*(kdx-(idx-1))\n",
    "                    break\n",
    "\n",
    "    return ent"
   ]
