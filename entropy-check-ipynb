    "def get_avg_min_variance(v, h):\n",
    "    v = np.array(v)\n",
    "    mins = []\n",
    "    for jdx in range(h.shape[0]):\n",
    "        test = h[jdx,:,0]\n",
    "#         print(test.shape)\n",
    "#         print(h[jdx].shape, v[jdx].shape)\n",
    "        for idx in range(test.shape[0]-1):\n",
    "            \n",
    "            if test[idx] > test[idx+1]+1:\n",
    "#                 print(v[jdx].shape)\n",
    "                mins.append(v[jdx,idx+1,0])\n",
    "    return np.mean(mins), mins\n",
